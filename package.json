{
  "name": "mroyale-editor",
  "version": "1.2.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ERmilburn02/mroyale-editor.git"
  },
  "author": "ERmilburn02",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ERmilburn02/mroyale-editor/issues"
  },
  "homepage": "https://github.com/ERmilburn02/mroyale-editor#readme",
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "discord-rpc": "^3.1.0",
    "electron-updater": "^4.2.5"
  },
  "build": {
    "appId": "com.ermilburn02.mroyale-editor",
    "forceCodeSigning": false,
    "win": {
      "publish": "github"
    },
    "extraResources": [
      "examples/"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "img/"
    ]
  }
}
